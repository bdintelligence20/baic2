{
  "name": "baic-website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.5.0",
    "react-scripts": "5.0.1",
    "react-slick": "^0.30.2",
    "slick-carousel": "^1.8.1",
    "styled-components": "^6.1.16",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:prerender": "react-scripts build && react-snap",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "optimize-images": "node scripts/optimizeImages.js",
    "build:optimized": "npm run optimize-images && npm run build"
  },
  "reactSnap": {
    "include": [
      "/",
      "/about/company-overview",
      "/vehicles/models/x55",
      "/vehicles/models/x55-plus",
      "/vehicles/models/x55-dynamic",
      "/vehicles/models/b40-plus",
      "/vehicles/models/b40-honor-edition",
      "/vehicles/promotions/x55",
      "/vehicles/promotions/x55-plus",
      "/vehicles/promotions/x55-dynamic",
      "/vehicles/promotions/b40-plus",
      "/vehicles/promotions/b40-honor-edition",
      "/find-dealer",
      "/book-test-drive",
      "/contact",
      "/finance",
      "/owners"
    ],
    "skipThirdPartyRequests": true,
    "cacheAjaxRequests": false,
    "preloadImages": false,
    "inlineCss": false,
    "minifyHtml": false,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--disable-dev-shm-usage",
      "--disable-accelerated-2d-canvas",
      "--no-first-run",
      "--no-zygote",
      "--disable-gpu"
    ],
    "crawl": false,
    "waitFor": 2000,
    "puppeteerExecutablePath": null,
    "puppeteerLaunchOptions": {
      "ignoreHTTPSErrors": true,
      "args": [
        "--no-sandbox",
        "--disable-setuid-sandbox",
        "--disable-dev-shm-usage"
      ]
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "react-snap": "^1.23.0",
    "sharp": "^0.34.3"
  }
}
